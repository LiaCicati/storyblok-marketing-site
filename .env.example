# Storyblok Preview/Draft token (required for server-side fetching)
# Found in: Storyblok Space > Settings > Access Tokens
STORYBLOK_API_TOKEN=your_preview_token_here

# Same token exposed to the client for the Storyblok Bridge / Visual Editor
NEXT_PUBLIC_STORYBLOK_API_TOKEN=your_preview_token_here

# Storyblok Personal Access Token (required for the setup script)
# Generate at: https://app.storyblok.com/#/me/account?tab=token
STORYBLOK_PERSONAL_TOKEN=your_personal_access_token_here

# Your Storyblok Space ID (required for the setup script)
# Found in: Storyblok Space > Settings > General
STORYBLOK_SPACE_ID=your_space_id_here
